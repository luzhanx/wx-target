page {
  height: 100%;
  background: #f8f8f8;
}

.ranking {
  align-items: center;
}

.ranking .title {
  width: 100%;
  font-size: 48rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx 0;
  z-index: 999;
  left: 0;
  top: 0;
  background: #fff;
}

.ranking .title .text {
  display: flex;
  position: relative;
}

.ranking .title .text view {
  padding: 0 14rpx;
  position: relative;
}

.ranking .title .text view:after {
  content: '';
  position: absolute;
  width: 2rpx;
  height: 60%;
  background: #000;
  right: 0;
  top: 25%;
}

.ranking .title .text .text1 {
  color: #82bed2;
}

.ranking .title .text .text2 {
  color: #f69581;
}

.ranking .title .text .text3 {
  color: #febb50;
}

.ranking .title .text:before {
  content: '';
  position: absolute;
  width: 2rpx;
  height: 60%;
  background: #000;
  left: 0;
  top: 25%;
}

.ranking .title .ranking_t1 {
  width: 22rpx;
  height: 37rpx;
  position: relative;
  top: 10rpx;
  left: -5rpx;
  margin-left: 50rpx;
}

.ranking .title .ranking_t2 {
  width: 63rpx;
  height: 69rpx;
  position: relative;
  left: -25rpx;
  top: -10rpx;
}

.ranking .msg {
  width: 100%;
  font-size: 24rpx;
  display: flex;
  top: 84rpx;
  left: 0;
  z-index: 99;
  background: #fff;
  box-shadow: 0 5rpx 10rpx 0 rgba(0, 0, 0, 0.06);
}

.ranking .msg text {
  padding: 5rpx 40rpx;
}

.ranking .msg:before {
  content: '';
  flex: 1;
  width: 100%;
  height: 2rpx;
  background: #dcdcdc;
  margin-top: 20rpx;
}

.ranking .msg:after {
  content: '';
  flex: 1;
  width: 100%;
  height: 2rpx;
  background: #dcdcdc;
  margin-top: 20rpx;
}

.ranking .ranking_c {
  padding-top: 20rpx;
  padding-bottom: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
}

.ranking .ranklist {
  width: 100%;
  background: #f8f8f8;
  position: relative;
  margin-top: 3rpx;
  padding-bottom: 100rpx;
}

.ranking .ranklist .bg2 {
  width: 100%;
  height: 191rpx;
  position: absolute;
  top: -60rpx;
}

.ranking .ranklist .rank {
  margin: 0 20rpx;
  background: #fff;
  height: 150rpx;
  margin-bottom: 20rpx;
  border-radius: 16rpx;
  box-shadow: 0 0 5rpx #eee;
  position: relative;
  display: flex;
  align-items: center;
  padding: 10rpx 20rpx;
}

.ranking .ranklist .rank .iinfo {
  display: flex;
  flex: 1;
  align-items: center;
}

.ranking .ranklist .rank .avatar {
  width: 125rpx;
  height: 124rpx;
  border-radius: 50%;
  margin: 0 20rpx;
}

.ranking .ranklist .rank .infos {
  flex: 1;
}

.ranking .ranklist .rank .infos .username {
  font-size: 28rpx;
  font-weight: bold;
  margin-bottom: 4rpx;
  height: 40rpx;
}

.ranking .ranklist .rank .infos .info, .ranking .ranklist .rank .infos .date {
  font-size: 22rpx;
  color: #818181;
}

.ranking .ranklist .rank .ttip {
  color: #edbb1b;
  font-size: 28rpx;
}

.case {
  position: relative;
  box-shadow: 0 0 10rpx #ccc;
  padding: 40rpx 14rpx 50rpx;
  margin: 0 26rpx;
  background: #fff;
}

.case_button {
  margin-bottom: 40rpx;
}

.case_img {
  width: 328rpx;
  height: 93rpx;
}

.target_s {
  margin-right: 14rpx;
}

.case_list {
  background: #f3f3f3;
  border-radius: 15rpx;
  height: 164rpx;
  line-height: 164rpx;
  display: flex;
  align-items: center;
  padding: 0 24rpx;
  margin-bottom: 20rpx;
}
.case_list:nth-last-child(1){
  margin-bottom: 0;
}

.case_list_img {
  width: 116rpx;
  height: 116rpx;
  float: left;
  margin-right: 20rpx;
  border-radius: 50%;
  overflow: hidden;
}

.case_left {
  width: 426rpx;
  border-right: 1px solid #dcdcdc;
}

.case_left image {
  width: 116rpx;
  height: 116rpx;
}

.case_title {
  font-size: 28rpx;
  color: #000;
  display: block;
}

.case_content {
  font-size: 24rpx;
  color: #7f7f7f;
}

.case_word {
  float: left;
  width: 282rpx;
  line-height: 40rpx;
  padding-right: 40rpx;
}

.case_right {
  float: right;
  /* display: flex; */
  align-items: center;
  text-align: center;
  flex-direction: column;
}

.case_right .num {
  width: 200rpx;
  font-size: 30rpx;
  color: #646464;
  height:76rpx;
}

.red {
  color: #f00;
}

.fashion {
  width: 156rpx;
  height: 54rpx;
}
